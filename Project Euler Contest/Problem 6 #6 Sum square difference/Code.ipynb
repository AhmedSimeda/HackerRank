   "source": [
    "T = int(input())\n",
    "\n",
    "for _ in range(T):\n",
    "    limit = int(input())\n",
    "    square_of_sum = (limit*(1+limit)//2)**2\n",
    "    sum_of_square = limit*(limit+1)*(2*limit+1)//6\n",
    "    print(abs(square_of_sum - sum_of_square))"
   ]
