    "s = input()\n",
    "cap = True\n",
    "for c in range(len(s)):\n",
    "    if s[c].isalnum() and cap:\n",
    "        s = s[:c] + s[c].upper() + s[c+1:]\n",
    "        cap = False\n",
    "    elif s[c] == ' ':\n",
    "        cap = True\n",
    "\n",
    "print(s)"
