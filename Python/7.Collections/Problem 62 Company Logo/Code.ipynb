    "from collections import Counter\n",
    "\n",
    "OCC = Counter(input())\n",
    "\n",
    "# the bigger the occurences the lower (-C[x])\n",
    "first_three = sorted(OCC, key=lambda x: (-OCC[x], x))[:3]\n",
    "\n",
    "for letter in first_three:\n",
    "    print(letter, OCC[letter])"
   ]
