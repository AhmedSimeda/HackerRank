    "from collections import namedtuple\n",
    "\n",
    "N = int(input())\n",
    "cols_names = input()\n",
    "\n",
    "marks = namedtuple('student', cols_names)\n",
    "Sum   = 0\n",
    "for n in range(N):   \n",
    "    mark = input().split()\n",
    "    record = marks(*mark)\n",
    "    Sum += int(record.MARKS)\n",
    "\n",
    "print(Sum/N)"
   ]
